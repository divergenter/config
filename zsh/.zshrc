export PATH=~/bin:/yun/fisherFly/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/yun/fisherFly/scripts
export ANDROID_ADB_SERVER_PORT=56789
autocutsel -f
